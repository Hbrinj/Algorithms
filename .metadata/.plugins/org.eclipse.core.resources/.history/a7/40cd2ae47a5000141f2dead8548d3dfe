
public class euler7 {
	public static void main(String argv[]){
		int count = 0;
		int TheOne = 1001;
		for(int i = 1; ;i++){
			if(isPrime((i*6)-1)){
				count++;
				if(count > TheOne){
					//System.out.println(i*6-1);
					break;
				}
			}
			if(isPrime((i*6)+1)){
				count++;
				if(count > TheOne){
					//System.out.println(i*6-1);
					break;
				}
			}
		}
	}
	
	public static boolean isPrime(int x){
		if(x % 2 == 0) return false;
		if(x % 3 == 0) return false;
		System.out.println(x);
		return true;
	}
}
